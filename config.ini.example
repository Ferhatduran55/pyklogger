[LOGGER]
## Log file settings
# Log file size in bytes
max_file_size = 1048576

# Log file save path
log_folder = logs

# Log file name format
log_file_format = log_%%(file_date_format).log

# Log file date format
log_file_date_format = %%Y-%%m-%%d_%%H-%%M-%%S

# Log file encoding format
log_format = [%%(date_format) %%(level)]: %%(word)

# Log file encoding date format
log_date_format = %%Y-%%m-%%d %%H:%%M:%%S-%%f

# Log file level
log_level = DEBUG